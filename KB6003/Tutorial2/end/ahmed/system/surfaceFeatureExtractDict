    {
	// Identify a feature when angle between faces < includedAngle
	// - 0  : selects no edges
	// - 180: selects all edges
	includedAngle       150;

    }

    // Write options
    // Write features to obj format for postprocessing
	writeObj                yes;
}

support.stl
{
    // How to obtain raw features (extractFromFile || extractFromSurface)
