FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            zeroGradient;
    }
    outlet
    {
        type            fixedValue;
        value           uniform 0;
    }
    ground
    {
        type            zeroGradient;
    }
    top
    {
        type            symmetry;
    }
    "side.*"
    {
        type            symmetry
    }
    BW
    {
        type            zeroGradient;
    }
    FLOOR
    {
        type            zeroGradient;
    }
    FLWh
    {
        type            zeroGradient;
    }
    FRWh
    {
        type            zeroGradient;
    }
    FW
    {
        type            zeroGradient;
    }
    RLWh
    {
        type            zeroGradient;
    }
    RRWh
    {
        type            zeroGradient;
    }
    RW
    {
        type            zeroGradient;
    }
}

